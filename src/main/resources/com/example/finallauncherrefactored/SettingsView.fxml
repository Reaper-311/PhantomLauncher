<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="376.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finallauncherrefactored.SettingsController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Settings">
         <font>
            <Font name="DIN Condensed Bold" size="76.0" />
         </font>
      </Label>
      <Button fx:id="btn_Confirm" layoutX="14.0" layoutY="359.0" mnemonicParsing="false" onAction="#btn_handleConfirm" text="Confirm">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_goBack" layoutX="307.0" layoutY="14.0" mnemonicParsing="false" onAction="#btn_handleGoBack" text="Go Back">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_about" layoutX="87.0" layoutY="359.0" mnemonicParsing="false" onAction="#btn_handleAbout" text="About">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Button>
      <Label layoutX="14.0" layoutY="112.0" prefHeight="45.0" prefWidth="122.0" text="Stay Signed In?">
         <font>
            <Font name="DIN Condensed Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="btn_SignInEnable" layoutX="136.0" layoutY="121.0" mnemonicParsing="false" onAction="#btn_handleSignInEnable" text="Enable">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btn_SignInDisable" layoutX="204.0" layoutY="121.0" mnemonicParsing="false" onAction="#btn_handleSignInDisable" text="Disable">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="label_SignedInStatus" layoutX="274.0" layoutY="126.0" prefHeight="18.0" prefWidth="88.0" text="Status:">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="label_PFPStatus" layoutX="160.0" layoutY="173.0" prefHeight="91.0" prefWidth="114.0" textFill="RED" wrapText="true">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
