<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finallauncherrefactored.MainMenuPG2Controller">
   <children>
      <Label layoutX="14.0" prefHeight="57.0" prefWidth="200.0" text="Welcome!">
         <font>
            <Font name="DIN Condensed Bold" size="60.0" />
