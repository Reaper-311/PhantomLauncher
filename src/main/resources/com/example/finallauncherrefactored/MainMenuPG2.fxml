      <Label layoutX="210.0" layoutY="79.0" prefHeight="36.0" prefWidth="180.0" text="Class Demo Projects">
         <font>
            <Font name="DIN Condensed Bold" size="28.0" />
         </font>
      </Label>
      <Button fx:id="btn_nextPage" layoutX="521.0" layoutY="361.0" mnemonicParsing="false" onAction="#btn_handleNextPage" text="-&gt;" />
