<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finallauncherrefactored.SignInController">
   <children>
      <Label layoutX="55.0" layoutY="34.0" prefHeight="83.0" prefWidth="196.0" text="Phantom">
         <font>
            <Font name="Arial Black" size="28.0" />
         </font>
      </Label>
      <Label layoutX="55.0" layoutY="158.0" text="Login or Create an Account!" />
      <Label layoutX="233.0" layoutY="76.0" text="Created by: Jacob Johnson &amp; Gabriel Lopez" />
      <TextField fx:id="tField_Username" layoutX="55.0" layoutY="188.0" promptText="Enter Username" />
      <PasswordField fx:id="pField_Password" layoutX="55.0" layoutY="237.0" promptText="Enter Password" />
      <Button fx:id="btn_Confirm" layoutX="215.0" layoutY="276.0" mnemonicParsing="false" onAction="#onBtnConfirm" text="Confirm">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font></Button>
      <Button fx:id="btn_CreateAccount" layoutX="215.0" layoutY="326.0" mnemonicParsing="false" onAction="#btn_OnCreateAccount" text="Create Account">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font></Button>
      <Label fx:id="label_Error" layoutX="285.0" layoutY="162.0" prefHeight="76.0" prefWidth="134.0" textFill="RED" />
      <Button fx:id="btn_Guest" layoutX="419.0" layoutY="276.0" mnemonicParsing="false" onAction="#btn_OnGuest" text="Sign In as Guest">
         <font>
            <Font name="DIN Condensed Bold" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
